 <main class="container mt-5">
  <h1 class="mb-4">login</h1>
  <form class="tech-login-form">
    <div class="mb-3">
      <label for="username" class="form-label">username</label>
      <input type="text" class="form-control" id="username-tech-login" placeholder="Enter your username" />
    </div>
    <div class="mb-3">
      <label for="password" class="form-label">password</label>
      <input type="password" class="form-control" id="password-tech-login" placeholder="Enter your password" />
    </div>
    <button type="submit" class="btn btn-primary">login</button>
  </form>
</main>